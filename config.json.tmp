{
  "production": {
    "appEnv": "production",
    "appHost": "localhost",
    "appPort": 3000,
    "redisPort": 6379,
    "redisHost": "localhost",
    "redisDatabase": 0,
    "errorHandlerOptions": {"dumpExceptions": false, "showStack": false}
  },
  "development": {
    "appEnv": "dev",
    "appHost": "localhost",
    "appPort": 3001,
    "redisPort": 6379,
    "redisHost": "localhost",
    "redisDatabase": 1,
    "errorHandlerOptions": {"dumpExceptions": true, "showStack": true}
  },
  "lastfm": {
    "key": "my_lastfm_key",
    "secret": "my_lastfm_secret"
  }
}
